	<queryString>
		<![CDATA[SELECT
     CONTRATOS.CONTRATO AS "CONTRATOS_CONTRATO",

     CONTRATOS.FECHAACTAAVANCE AS "CONTRATOS_FECHAACTAAVANCE",
     CONTRATOS.FKTERCERO AS "CONTRATOS_FKTERCERO",
     CONTRATOS.OBJETO AS "CONTRATOS_OBJETO",
     CONTRATOS.PLAZO AS "CONTRATOS_PLAZO",
     CONTRATOS.UNIDADDURACION AS "CONTRATOS_UNIDADDURACION",
     CONTRATOS.VALOR AS "CONTRATOS_VALOR",
     CONTRATOS.VALORSINIVA AS "CONTRATOS_VALORSINIVA",
     CONTRATOS.FACULTAD AS "CONTRATOS_FACULTAD",
     CONTRATOS.FECHASUSCRIPCION AS "CONTRATOS_FECHASUSCRIPCION",
     CONTRATOS.PLANADQUISICION AS "CONTRATOS_PLANADQUISICION",
     CONTRATOS.VALORLETRAS AS "CONTRATOS_VALORLETRAS",
     TERCEROS.ID AS "TERCEROS_ID",
     TERCEROS.NOMBRE AS "TERCEROS_NOMBRE",
     TERCEROS.DIRECCION AS "TERCEROS_DIRECCION",
     TERCEROS.TELEFONO AS "TERCEROS_TELEFONO",
     TERCEROS.EMAIL AS "TERCEROS_EMAIL",
     TERCEROS.NOMBREREPRESENTANTE AS "TERCEROS_NOMBREREPRESENTANTE",
     TERCEROS.IDREPRESENTANTE AS "TERCEROS_IDREPRESENTANTE"
FROM
     CONTRATOS LEFT JOIN TERCEROS ON CONTRATOS.FKTERCERO = TERCEROS.ID || ' - ' || TERCEROS.NOMBRE
WHERE
     CONTRATOS.CONTRATO = $P{CONTRATO}]]>
	</queryString>
