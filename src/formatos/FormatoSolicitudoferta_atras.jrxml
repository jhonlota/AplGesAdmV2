				</textElement>
				<text><![CDATA[1. IMPUESTOS DEDUCCIONES (1)]]></text>
			</staticText>
			<staticText>
				<reportElement uuid="c22532a3-630d-44a1-abfb-161de7f64e3d" style="table" mode="Transparent" x="122" y="102" width="316" height="15" forecolor="#000000" backcolor="#FFFFFF"/>
				<textElement textAlignment="Left" verticalAlignment="Top" rotation="None" markup="none">
