	</parameter>
	<queryString>
		<![CDATA[SELECT
     f_soportescontratos('Registro', $P{FKCONTRATO}) AS "F_SOPORTESCONTRATOS",
     SOPORTESCONTRATOS.FECHA AS "SOPORTESCONTRATOS_FECHA"
FROM
     SOPORTESCONTRATOS
WHERE
     SOPORTESCONTRATOS.FKCONTRATO = $P{FKCONTRATO}
     AND SOPORTESCONTRATOS.TIPO = 'Certificado'
