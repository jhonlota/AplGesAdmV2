	</parameter>
	<queryString>
		<![CDATA[SELECT
     f_soportescontratos('Certificado', $P{FKCONTRATO}) AS "F_SOPORTESCONTRATOS",
     SOPORTESCONTRATOS.FECHA AS "SOPORTESCONTRATOS_FECHA"
FROM
     SOPORTESCONTRATOS
