	<subDataset name="DATASETPLANESTRATEGICO" uuid="12c6c91f-8db4-4fe1-a85e-1c2369c312fa">
		<parameter name="FKCONTRATOTABLE" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT
     *
FROM
     PLANESTRATEGICOCONTRATOS
WHERE
     FKCONTRATO = $P{FKCONTRATOTABLE}]]>
		</queryString>
		<field name="fkcontrato" class="java.lang.String"/>
		<field name="id" class="java.lang.Integer"/>
		<field name="eje" class="java.lang.String"/>
		<field name="estrategia" class="java.lang.String"/>
		<field name="programa" class="java.lang.String"/>
		<field name="proyecto" class="java.lang.String"/>
		<field name="subgrupo" class="java.lang.String"/>
		<field name="cinfo" class="java.lang.String"/>
	</subDataset>
	<subDataset name="DATASETCUBS" uuid="3f3b276e-04ec-4c59-aec4-3f34ad94bfe8">
		<parameter name="FKCONTRATOTABLE" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT DISTINCT CASE
		WHEN CAST(CLASIFICADORUNSPSC.CODIGOPRODUCTO AS INT) < 999999
			THEN CLASIFICADORUNSPSC.NOMBREPRODUCTO
			ELSE CLASIFICADORUNSPSC.NOMBRECLASE
		END AS NOMBREPRODUCTO,
		CUBS.DESCRIPCION,
		CUBS.CANTIDAD,
		CONTRATOS.PLAZO,
		CONTRATOS.UNIDADDURACION
FROM
CLASIFICADORUNSPSC AS CLASIFICADORUNSPSC
JOIN CUBS AS CUBS
ON (CUBS.CODIGO = CLASIFICADORUNSPSC.NOMBREPRODUCTO || ' [' || CLASIFICADORUNSPSC.CODIGOPRODUCTO  || ']'
	OR CUBS.CODIGO = CLASIFICADORUNSPSC.NOMBRECLASE || ' [' || CLASIFICADORUNSPSC.CODIGOCLASE  || ']')

JOIN CONTRATOS AS CONTRATOS
ON CONTRATOS.CONTRATO = CUBS.FKCONTRATO
AND CUBS.FKCONTRATO = $P{FKCONTRATOTABLE}
WHERE CLASIFICADORUNSPSC.CODIGOGRUPO <> 'F']]>
		</queryString>
		<field name="nombreproducto" class="java.lang.String"/>
		<field name="descripcion" class="java.lang.String"/>
		<field name="cantidad" class="java.lang.Long"/>
		<field name="plazo" class="java.lang.String"/>
		<field name="unidadduracion" class="java.lang.String"/>
	</subDataset>
