	<subDataset name="DATASETFUNDAMENTO" uuid="f72bcdcc-bdb3-4223-b22b-63d0a527d58f">
		<parameter name="CONTRATO" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT MODALIDAD.FUNDAMENTO
