	<subDataset name="DATASETCLASIFICADOR" uuid="e99ddc5e-e911-46af-be43-a4bf51a3b27c">
		<parameter name="CONTRATOTABLE" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT DISTINCT  CLASIFICADORUNSPSC.CODIGOSEGMENTO,
	       CLASIFICADORUNSPSC.NOMBRESEGMENTO,
	       CLASIFICADORUNSPSC.CODIGOFAMILIA,
	       CLASIFICADORUNSPSC.NOMBREFAMILIA,
                 CLASIFICADORUNSPSC.CODIGOCLASE,
	       CLASIFICADORUNSPSC.NOMBRECLASE,
	       CLASIFICADORUNSPSC.NOMBREGRUPO,
                 CLASIFICADORUNSPSC.CODIGOGRUPO
FROM CLASIFICADORUNSPSC AS CLASIFICADORUNSPSC
JOIN CUBS AS CUBS
ON (CUBS.CODIGO = CLASIFICADORUNSPSC.NOMBREPRODUCTO || ' [' || CLASIFICADORUNSPSC.CODIGOPRODUCTO  || ']'
	OR CUBS.CODIGO = CLASIFICADORUNSPSC.NOMBRECLASE || ' [' || CLASIFICADORUNSPSC.CODIGOCLASE  || ']')
AND CUBS.FKCONTRATO = $P{CONTRATOTABLE}]]>
		</queryString>
		<field name="codigosegmento" class="java.lang.String"/>
		<field name="nombresegmento" class="java.lang.String"/>
		<field name="codigofamilia" class="java.lang.String"/>
		<field name="nombrefamilia" class="java.lang.String"/>
		<field name="codigoclase" class="java.lang.String"/>
		<field name="nombreclase" class="java.lang.String"/>
		<field name="nombregrupo" class="java.lang.String"/>
		<field name="codigogrupo" class="java.lang.String"/>
	</subDataset>
